  export NODE_PATH="/Users/ianderrington/git/supernal-nova/families/supernal-coding/node_modules/.pnpm/@supernal+compliance-cards@https+++codeload.github.com+ianderrington+compliance-cards+t_d158cb80bc7f684cc784e1fe12f8c768/node_modules/@supernal/compliance-cards/bin/node_modules:/Users/ianderrington/git/supernal-nova/families/supernal-coding/node_modules/.pnpm/@supernal+compliance-cards@https+++codeload.github.com+ianderrington+compliance-cards+t_d158cb80bc7f684cc784e1fe12f8c768/node_modules/@supernal/compliance-cards/node_modules:/Users/ianderrington/git/supernal-nova/families/supernal-coding/node_modules/.pnpm/@supernal+compliance-cards@https+++codeload.github.com+ianderrington+compliance-cards+t_d158cb80bc7f684cc784e1fe12f8c768/node_modules/@supernal/node_modules:/Users/ianderrington/git/supernal-nova/families/supernal-coding/node_modules/.pnpm/@supernal+compliance-cards@https+++codeload.github.com+ianderrington+compliance-cards+t_d158cb80bc7f684cc784e1fe12f8c768/node_modules:/Users/ianderrington/git/supernal-nova/families/supernal-coding/node_modules/.pnpm/node_modules:$NODE_PATH"
