{
  "version": 2,
  "name": "supernal-coding-docs",
  "buildCommand": "pnpm run build",
  "outputDirectory": "build",
  "installCommand": "pnpm install --frozen-lockfile",
  "public": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/dashboard-live",
      "destination": "/dashboard-live"
    }
  ]
}
